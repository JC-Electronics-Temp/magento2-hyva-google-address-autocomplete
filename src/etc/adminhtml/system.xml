<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Config:etc/system_file.xsd">
    <system>
        <tab id="jcelectronics" sortOrder="800" translate="label">
            <label>JC-Electronics</label>
        </tab>
        <section id="google_address_autocomplete" translate="label" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
            <label>Google Address Autocomplete</label>
            <tab>jcelectronics</tab>
            <resource>JcElectronics_HyvaGoogleAddressAutocomplete::config_exact</resource>
            <group id="general" translate="label" type="text" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
                <label>General Settings</label>
                <field id="enabled" translate="label" type="select" sortOrder="10" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>Enabled</label>
                    <source_model>Magento\Config\Model\Config\Source\Yesno</source_model>
                </field>
                <field id="api_key" translate="label" sortOrder="120" showInDefault="1" showInWebsite="0" showInStore="0">
                    <label>API Key</label>
                </field>
            </group>
        </section>
    </system>
</config>
